    "class Person:\n",
    "    def __init__(self, weight, age):\n",
    "        self.weight = weight\n",
    "        self.age = age\n",
    "\n",
    "    def __add__(self, other):\n",
    "        return self.weight + other.weight\n",
    "\n",
    "\n",
    "p1 = Person(75, 40)\n",
    "p2 = Person(65, 45)\n",
    "\n",
    "print(p1 + p2)\n"
